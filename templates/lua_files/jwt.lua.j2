local j = require "guardjwt"
local validators = require "resty.jwt-validators"

j.GuardJWT.verify(
    {
{% for verify in verifications %}
    {{ verify.indicatior }} = {
        validators = validators.equals_any_of({ {{ verify.equals }} }),
        header = "{{ verify.header }}"
    }
{% endfor %}
    },
    {
    secret = "{{ secret }}",
    is_token_mandatory = {{ is_token_mandatory | default('true') }},
    clear_authorization_header = {{ clear_authorization_header | default('true') }}
    }
)